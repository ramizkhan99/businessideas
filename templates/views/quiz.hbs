  <script>
    // console.log("hey")
    var flag
    $.ajax({
            type:'GET',
            url:'https://business-ideas-users-api.herokuapp.com/users/me',
            headers: {'Authorization': `Bearer ${document.cookie}`},
            success:function(currentState){
              flag = currentState.flag
              var data = JSON.parse(localStorage.user)
              const company = data.user.company
              if (flag == "true") {
                location.href = '/question'
              }
              else{
              if(company=='samsung'){
                         location.href='/samsungsal'
                     }
                     else if(company=='ola'){
                         location.href='/olasal'
                     }
                     else if(company=='dlf'){
                         location.href='/dlfsal'
                     }
                     else if(company=='indiaoils'){
                         location.href='/indiaoilssal'
                     }
